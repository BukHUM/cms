VITE_APP_NAME="${APP_NAME}"

# Vite Development Server Configuration
# Development Environment
VITE_HOST=0.0.0.0
VITE_PORT=5223
VITE_HMR_HOST=localhost
VITE_CORS_ORIGINS=http://backend.local,http://localhost:8000

# Production Environment Example:
# VITE_HOST=0.0.0.0
# VITE_PORT=3000
# VITE_HMR_HOST=yourdomain.com
# VITE_CORS_ORIGINS=https://yourdomain.com,https://api.yourdomain.com

# Staging Environment Example:
# VITE_HOST=0.0.0.0
# VITE_PORT=3001
# VITE_HMR_HOST=staging.yourdomain.com
# VITE_CORS_ORIGINS=https://staging.yourdomain.com

# CORS Headers Configuration (Optional)
VITE_CORS_ALLOW_ORIGIN="*"
VITE_CORS_ALLOW_METHODS="GET, POST, PUT, DELETE, OPTIONS"
VITE_CORS_ALLOW_HEADERS="Content-Type, Authorization"
